{
  "name": "meu-projeto-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "test": "jest",
    "migrations:create": "node-pg-migrate -m infra/migrations --language ts create",
    "migrations:up": "dotenv -e .env -- ts-node --transpile-only ./node_modules/node-pg-migrate/bin/node-pg-migrate.js up -m infra/migrations",
    "migrations:test:up": "dotenv -e .env.test -- ts-node --transpile-only ./node_modules/node-pg-migrate/bin/node-pg-migrate.js up -m infra/migrations",
    "migrations:down": "dotenv -e .env -- ts-node --transpile-only ./node_modules/node-pg-migrate/bin/node-pg-migrate.js down -m infra/migrations",
    "migrations:test": "ts-node --transpile-only scripts/run-migrations.ts",
    "clear_tables": "psql -U gabrielrosa -d localdatabase -c \"TRUNCATE TABLE events, lotes RESTART IDENTITY CASCADE;\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
    "@types/pg": "^8.11.13",
    "@types/supertest": "^6.0.3",
    "dotenv-cli": "^8.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.19.2",
    "node-pg-migrate": "^7.9.1",
    "pg": "^8.14.1",
    "zod": "^3.24.3"
  }
}
